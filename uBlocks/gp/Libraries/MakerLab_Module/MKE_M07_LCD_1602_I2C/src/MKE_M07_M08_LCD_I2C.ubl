module MKE_M07_M08_LCD_I2C
author MinhKhanh210
version 1 0 
depends 'LCD Display' 
description ''

  spec ' ' 'M07/08 LCD I2C | Clear' 'M07/08 LCD I2C | Clear'
  spec ' ' 'M07/08 LCD I2C | Print' 'M07/08 LCD I2C | Print _ at Columm _ and Row _' 'str auto auto' 'MakerEDU' 1 1

to 'M07/08 LCD I2C | Clear' {
  LCD1602_clear
}

to 'M07/08 LCD I2C | Print' text col row {
  LCD1602_write_field text row col (col + (size text))
}

